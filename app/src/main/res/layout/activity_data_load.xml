<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:gravity="center"
    android:layout_height="match_parent"
    tools:context=".data_load">

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="GryffindoreYukle"
        android:text="Gryffindore Yükle"
        android:textSize="18sp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="16dp" />
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Slytherin Yükle"
        android:textSize="18sp"
        android:onClick="SlytherinYukle"
        android:layout_marginTop="30dp"
        tools:ignore="MissingConstraints" />
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="HufflePuff Yükle"
        android:textSize="18sp"
        android:onClick="HufflePuffYukle"
        android:layout_marginTop="30dp"
        tools:ignore="MissingConstraints" />
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="RavenClaw Yükle"
        android:textSize="18sp"
        android:onClick="RavenClawYukle"
        android:layout_marginTop="30dp"
        tools:ignore="MissingConstraints" />
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Back Yükle"
        android:textSize="18sp"
        android:onClick="backYukle"
        android:layout_marginTop="30dp"
        tools:ignore="MissingConstraints" />

